{
    "name": "pro-rack",
    "version": "1.0.0",
    "author": "Suresh Jangale",
    "scripts": {
        "build": "cd client && npm run build",
        "install-backend": "cd backend && npm install --dev",
        "install-client": "cd client && npm install --dev",
        "server": "cd backend && npm run start:production",
        "client": "cd client && npm run start:production",
        "start": "npm run server",
        "postinstall": "(cd backend && npm install --dev); (cd client && npm install --dev)",
        "heroku-postbuild": "npm run build",
        "start:production": "cd backend && npm run start:production && cd ../client && npm run start:production"
    },
    "engines": {
        "node": "12.14.0"
    },
    "devDependencies": {
        "@webpack-cli/serve": "^1.3.1",
        "concurrently": "^6.2.0"
    },
    "dependencies": {
        "concurrently": "^6.2.0"
    }
}